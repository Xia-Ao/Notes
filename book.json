{
  "title": "前端知识体系",
  "description": "有关前端知识总结以及面试题目",
  "language": "zh",
  "author": "xiaao",
  "plugins": [
    "-highlight",
    "prism",
    "prism-themes",
    "highlight-pro"
  ],
  "pluginsConfig": {
    "github": {
      "url": "https://github.com/Xia-Ao"
    },
    "fontsettings": {
      "theme": "white",
      "family": "sans",
      "size": 3
    },
    "highlight-pro": {
      "theme": "vs2015",
      "map": {
        "shell": "bash",
        "sh": "bash",
        "js": "javascript"
      }
    },
    "prism": {
      "css": [
        "prism-themes/themes/prism-base16-ateliersulphurpool.light.css"
      ]
    },
    "theme-default": {
      "showLevel": false
    }
  }
}